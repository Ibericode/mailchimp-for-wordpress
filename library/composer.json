{
  "name": "ibericode/mc4wp-library",
  "type": "wordpress-plugin",
  "require": {
    "xrstf/composer-php52": "1.*",
    "composer/installers": "~1.0"
  },
  "scripts": {
    "post-install-cmd": [
      "xrstf\\Composer52\\Generator::onPostInstallCmd"
    ],
    "post-update-cmd": [
      "xrstf\\Composer52\\Generator::onPostInstallCmd"
    ],
    "post-autoload-dump": [
      "xrstf\\Composer52\\Generator::onPostInstallCmd"
    ]
  },
  "autoload": {
    "classmap": [
      "src/"
    ],
    "files": ["src/functions.php" ]
  }
}